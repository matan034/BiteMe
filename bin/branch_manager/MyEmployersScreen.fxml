<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="650.0" prefWidth="1010.0" stylesheets="@../general/GeneralStyleSheet.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="managment.MyEmployersController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" />
      <VBox fx:id="waiting_for_approval_vbox" prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label styleClass="lbl" stylesheets="@../general/GeneralStyleSheet.css" text="Waiting For Approval">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <Accordion fx:id="waiting_for_approval_accordion" />
         </children>
      </VBox>
      <Separator prefWidth="200.0" style="-fx-background-color: #F7C665;" />
      <VBox fx:id="approved_vbox" prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label styleClass="lbl" stylesheets="@../general/GeneralStyleSheet.css" text="Approved">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <Accordion fx:id="approved_accordion" />
         </children>
      </VBox>
   </children>
</VBox>
